{
  "name": "auth-router-ssr.com",
  "author": "Kaung Myat Lin",
  "scripts": {
    "compile:scss": "sass --no-source-map components/layout/auth/auth_form.module.scss:components/layout/auth/compilemain.css --style expanded",
    "prefix:css": "postcss components/layout/auth/compilemain.css -u autoprefixer -b \"last 10 versions\" -o components/layout/auth/main.prefixed.css",
    "compress:css": "sass --no-source-map components/layout/auth/main.prefixed.css:components/layout/auth/main.min.css",
    "build:cmpl:prfx:cmprs": "npm-run-all compile:scss prefix:css compress:css --watch",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "formik": "^2.2.9",
    "mongodb": "^4.11.0",
    "next": "12.3.1",
    "next-auth": "^4.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.2.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/react": "18.0.25",
    "autoprefixer": "^10.4.13",
    "eslint": "8.25.0",
    "eslint-config-next": "12 .3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.1.0",
    "sass": "^1.56.1",
    "typescript": "^4.9.3"
  }
}
